<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="@Url.Content("~/Content/ShooesStayle.css")" rel="stylesheet" type="text/css" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Anton&family=Bebas+Neue&family=Lobster&family=Mochiy+Pop+One&family=Playfair+Display:ital,wght@0,500;0,700;0,800;0,900;1,700;1,900&display=swap" rel="stylesheet">
    <style>
        .mylogo {
            width: 50px;
           
        
        }
        #logos {
            margin-left: 350px;
        }
    </style>
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("HOME PAGE", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Shoes", "ShowShoes", "Shoes")</li>
                    <li>@Html.ActionLink("Shirts", "ShowAllTshirt", "Clothing")</li>
                    <li>@Html.ActionLink("Pants", "ShowAllPants", "Clothing")</li>
                    <li>@Html.ActionLink("Equipment", "ShowAllSportsEquipments", "SportsEquipment")</li>
                    <li id="logos"><img src="~/Images/logo.png" class="mylogo"/> <img src="~/Images/לוגו טק.jpg" class="mylogo" /> <img src="~/Images/לוגו.jpg" class="mylogo"/></li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()

       
        <hr />
        <footer>
           
            <a href="https://github.com/TikvaYosef" target="_blank"><img class="mylogo" src="data:image/png;base64," /> </a>
            <a href="https://www.linkedin.com/in/tikva-yosef-406bba223/" target="_blank"><img class="mylogo" src="data:image/png;base64," /> </a>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
